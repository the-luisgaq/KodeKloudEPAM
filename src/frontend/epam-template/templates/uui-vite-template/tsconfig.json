{
    "compilerOptions": {
        "esModuleInterop": true,
        "isolatedModules": true,
        "jsx": "react-jsx",
        "module": "CommonJS",
        "moduleResolution": "node",
        "noEmit": true,
        "resolveJsonModule": true,
        "strict": true,
        "strictNullChecks": false,
        "skipLibCheck": true,
        "target": "ES2021",
        "rootDir": ".",
        "types": [
            "vitest/globals",
            "vitest-dom/extend-expect",
            "vite/client",
            "vite-plugin-svgr/client"
        ]
    }
}
